<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pl.ydapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/yd_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/yd_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main2Activity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CompareIDActivity"
            android:launchMode="singleTask">
        </activity>
        <activity android:name=".ComfirmOutActivity"
            android:launchMode="singleTask">
        </activity>
        <activity android:name=".PartInActivity"
            android:launchMode="singleTask">
        </activity>
        <activity android:name=".PrintActivity"
            android:launchMode="singleTask">
        </activity>
    </application>

</manifest>